%html
  %head
    = partial("partials/head")
  %body.l--body

    %header.l--masthead.masthead.l--contain
      = partial("partials/masthead")

    %section.l--main.l--contain
      #main{:role => "main"}
        = yield

    %footer.footer
      .l--contain
        = partial("partials/footer")
  %script{:src => "http://ajax.googleapis.com/ajax/libs/jquery/1.7.0/jquery.min.js", :type => "text/javascript"}
  %script(src="javascripts/links.js")
